<tables>
<table>
<name>vtiger_marcas</name>
<sql><![CDATA[CREATE TABLE `vtiger_marcas` (
  `marcasid` int(11) DEFAULT NULL,
  `marcasno` varchar(50) DEFAULT NULL,
  `marcaname` varchar(255) DEFAULT NULL,
  `puedecomprar` varchar(3) DEFAULT NULL,
  `compra` varchar(3) DEFAULT NULL,
  `presentado` date DEFAULT NULL,
  `novedades` varchar(200) DEFAULT NULL,
  `accid` int(11) DEFAULT NULL,
  `ctoid` int(11) DEFAULT NULL,
  PRIMARY KEY (`marcasid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
<table>
<name>vtiger_marcascf</name>
<sql><![CDATA[CREATE TABLE `vtiger_marcascf` (
